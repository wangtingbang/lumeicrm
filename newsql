CREATE TABLE `car_buying` (
  `id` varchar(32) NOT NULL,
  `user_id` varchar(32) DEFAULT NULL,
  `Sales_Name` varchar(255) DEFAULT NULL,
  `Category` varchar(255) DEFAULT NULL,
  `Car_Description` varchar(255) DEFAULT NULL,
  `Makes` varchar(255) DEFAULT NULL,
  `Model` varchar(255) DEFAULT NULL, 
  `is_New` smallint(6) DEFAULT NULL COMMENT 'new, used',
  `Years` varchar(255) DEFAULT NULL COMMENT '选used就要填',
  `Mileages` varchar(255) DEFAULT NULL COMMENT '选used就要填',
  `MSRP` varchar(255) DEFAULT NULL COMMENT '选used就要填',
  `Color` varchar(255) DEFAULT NULL,
  `Exterior` varchar(255) DEFAULT NULL,
  `Interior` varchar(255) DEFAULT NULL,
  `comments` varchar(512) DEFAULT NULL,
  `Additional_Information` varchar(255) DEFAULT NULL,
  `Credit_Card_no` varchar(255) DEFAULT NULL,
  `Expiration_Date` varchar(255) DEFAULT NULL,
  `CVS` varchar(255) DEFAULT NULL,
  `Drivers_License` varchar(255) DEFAULT NULL,
  `Finance_Down_Payment` varchar(255) DEFAULT NULL,
  `Finance_Rate` varchar(255) DEFAULT NULL,
  `Finance_Terms` varchar(255) DEFAULT NULL,
  `Finance_Monthly_Pay` varchar(255) DEFAULT NULL,
  `Finance_comments` varchar(512) DEFAULT NULL,
  `Lease_Additional_File` varchar(255) DEFAULT NULL COMMENT '1	2	3	etc	(是否可以做成可以增加的类型）',
  `Lease_Down_Payment` varchar(255) DEFAULT NULL,
  `Lease_Terms` varchar(255) DEFAULT NULL,
  `Lease_Monthly_Pay` varchar(255) DEFAULT NULL,
  `Lease_Comments` varchar(512) DEFAULT NULL,
  `Trade_in` varchar(255) DEFAULT NULL COMMENT 'yes/no, 若为no以下都不用填',
  `Trade_in_Makes` varchar(255) DEFAULT NULL,
  `Trade_in_Model` varchar(255) DEFAULT NULL,
  `Trade_in_Years` varchar(255) DEFAULT NULL,
  `Trade_in_Mileage` varchar(255) DEFAULT NULL,
  `trade_in_value` varchar(255) DEFAULT NULL,
  `Trade_in_Color` varchar(255) DEFAULT NULL,
  `Trade_in_Exterior` varchar(255) DEFAULT NULL,
  `Trade_in_Interior` varchar(255) DEFAULT NULL,
  `delete_flag` tinyint(4) DEFAULT NULL,
  `create_user_id` varchar(32) DEFAULT NULL,
  `create_time` date NULL DEFAULT NULL,
  `update_user_id` varchar(32) DEFAULT NULL,
  `update_time` date NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `emergency_contact` (
  `id` varchar(32) NOT NULL,
  `user_id` varchar(32) DEFAULT NULL,
  `Sales_Name` varchar(32) DEFAULT NULL,
  `DOB` varchar(255) DEFAULT NULL,
  `chinese_id` varchar(255) DEFAULT NULL,
  `passport` varchar(255) DEFAULT NULL,
  `University` varchar(255) DEFAULT NULL,
  `School` varchar(255) DEFAULT NULL,
  `Department` varchar(255) DEFAULT NULL,
  `school_email` varchar(255) DEFAULT NULL,
  `Personal_email` varchar(255) DEFAULT NULL,
  `Wechat_ID` varchar(255) DEFAULT NULL,
  `chinese_home_address` varchar(255) DEFAULT NULL,
  `chinese_home_zip` varchar(255) DEFAULT NULL,
  `chinese_home_telephone` varchar(255) DEFAULT NULL,
  `chinese_home_cellphone` varchar(255) DEFAULT NULL,
  `usa_home_address` varchar(255) DEFAULT NULL,
  `usa_home_city` varchar(255) DEFAULT NULL,
  `usa_home_state` varchar(255) DEFAULT NULL,
  `usa_home_zip` varchar(255) DEFAULT NULL,
  `usa_home_telephone` varchar(255) DEFAULT NULL,
  `usa_home_cellphone` varchar(255) DEFAULT NULL,
  `father_chinese_name` varchar(255) DEFAULT NULL,
  `father_english_name` varchar(255) DEFAULT NULL,
  `father_address` varchar(255) DEFAULT NULL,
  `father_telephone` varchar(255) DEFAULT NULL,
  `father_cellphone` varchar(255) DEFAULT NULL,
  `father_wechat_id` varchar(255) DEFAULT NULL,
  `mother_chinese_name` varchar(255) DEFAULT NULL,
  `mother_english_name` varchar(255) DEFAULT NULL,
  `mother_address` varchar(255) DEFAULT NULL,
  `mother_telephone` varchar(255) DEFAULT NULL,
  `mother_cellphone` varchar(255) DEFAULT NULL,
  `mother_wechat_id` varchar(255) DEFAULT NULL,
  `Landlord_chinese_name` varchar(255) DEFAULT NULL,
  `Landlord_english_name` varchar(255) DEFAULT NULL,
  `Landlord_address` varchar(255) DEFAULT NULL,
  `Landlord_city` varchar(255) DEFAULT NULL,
  `Landlord_state` varchar(255) DEFAULT NULL,
  `Landlord_zip` varchar(255) DEFAULT NULL,
  `Landlord_telephone` varchar(255) DEFAULT NULL,
  `Landlord_cellphone` varchar(255) DEFAULT NULL,
  `Landlord_wechat_id` varchar(255) DEFAULT NULL,
  `Roommate_chinese_name` varchar(255) DEFAULT NULL,
  `Roommate_english_name` varchar(255) DEFAULT NULL,
  `Roommate_address` varchar(255) DEFAULT NULL,
  `Roommate_city` varchar(255) DEFAULT NULL,
  `Roommate_state` varchar(255) DEFAULT NULL,
  `Roommate_zip` varchar(255) DEFAULT NULL,
  `Roommate_telephone` varchar(255) DEFAULT NULL,
  `Roommate_cellphone` varchar(255) DEFAULT NULL,
  `Neighbor_chinese_name` varchar(255) DEFAULT NULL,
  `Neighbor_english_name` varchar(255) DEFAULT NULL,
  `Neighbor_address` varchar(255) DEFAULT NULL,
  `Neighbor_city` varchar(255) DEFAULT NULL,
  `Neighbor_state` varchar(255) DEFAULT NULL,
  `Neighbor_zip` varchar(255) DEFAULT NULL,
  `Neighbor_telephone` varchar(255) DEFAULT NULL,
  `Neighbor_cellphone` varchar(255) DEFAULT NULL,
  `Other_chinese_name` varchar(255) DEFAULT NULL,
  `Other_english_name` varchar(255) DEFAULT NULL,
  `Other_address` varchar(255) DEFAULT NULL,
  `Other_city` varchar(255) DEFAULT NULL,
  `Other_state` varchar(255) DEFAULT NULL,
  `Other_zip` varchar(255) DEFAULT NULL,
  `Other_telephone` varchar(255) DEFAULT NULL,
  `Other_cellphone` varchar(255) DEFAULT NULL,
  `special` tinyint(4) DEFAULT NULL,
  `special_comment` varchar(512) DEFAULT NULL,
  `total` int(10) DEFAULT 0,
  `used` int(10) DEFAULT 0,
  `expiration_date` date default null,
  `delete_flag` tinyint(4) DEFAULT NULL,
  `create_user_id` varchar(32) DEFAULT NULL,
  `create_time` timestamp NULL DEFAULT NULL,
  `update_user_id` varchar(32) DEFAULT NULL,
  `update_time` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

create table `files` (
  `id` varchar(32) not null,
  `user_id` varchar(32) default null,
  `service_id` varchar(32) default null,
  `file_name` varchar(255) default null,
  `uri` varchar(255) default null,
  `delete_flag` tinyint(4) default null,
  `create_user_id` varchar(32) default null,
  `create_time` date default null,
  `update_user_id` varchar(32) default null,
  `update_time` date default null,
  primary key (`id`)
) engine=innodb default charset=utf8;

create table `transaction` (
  `id` varchar(32) not null,
  `service_type` tinyint(3) default null,
  `user_id` varchar(32) default null,
  `service_id` varchar(32) default null,
  `delete_flag` tinyint(4) default null,
  `create_user_id` varchar(32) default null,
  `create_time` date default null,
  `update_user_id` varchar(32) default null,
  `update_time` date default null,
  primary key (`id`)
) engine=innodb default charset=utf8;

create table `notes` (
  `id` varchar(32) not null,
  `note_service_type` tinyint(3) default null,
  `user_id` varchar(32) default null,
  `service_id` varchar(32) default null,
  `content` varchar(512) default null,
  `delete_flag` tinyint(4) default null,
  `create_user_id` varchar(32) default null,
  `create_time` date default null,
  `update_user_id` varchar(32) default null,
  `update_time` date default null,
  primary key (`id`)
) engine=innodb default charset=utf8;

create table `profile` (
  `id` varchar(32) not null,
  `sales` varchar(32) default null,
  `name` varchar(128) default null,
  `gender` smallint(6) default null,
  `phone` varchar(64) default null,
  `email` varchar(128) default null,
  `wechat_id` varchar(64) default null,
  `street` varchar(256) default null,
  `city` varchar(64) default null,
  `state` varchar(64) default null,
  `zip_code` varchar(64) default null,
  `university` varchar(255) default null,
  `status` varchar(64) default null,
  `rating` varchar(64) default null,
  `potential_buying_date`date default null,
  `service` varchar(255) default null comment 'car buying, car selling, car renal, cellphone, airport pickup, temporary house, aim, emergency contact',
  `delete_flag` tinyint(4) default null,
  `create_user_id` varchar(32) default null,
  `create_time` date default null,
  `update_user_id` varchar(32) default null,
  `update_time` date default null,
  primary key (`id`)
) engine=innodb default charset=utf8;

